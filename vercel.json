{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    
    { "src": "/roblox/(.*)", "dest": "/api/robloxinfo/$1" },

    { "src": "/", "dest": "/index.html" },

    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/.*", "status": 404, "dest": "/404.html" }
  ]
}
